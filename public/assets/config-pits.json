{
  "heading": "REEFSCAPE - 2025 (Pit Scouting)",
  "logo": "test_scouting_logo.png",
  "skipTeamSelection": true,
  "pages": [
    {
      "name": "Team",
      "widgets": [
        {
          "name": "Team Name",
          "type": "text"
        },
        {
          "name": "Team Number",
          "type": "number"
        }
      ]
    },




    {
      "name": "Robot",
      "widgets": [

        {
          "name": "Height",
          "type": "number"
        },
        {
          "name": "Length",
          "type": "number"
        },
        {
          "name": "Width",
          "type": "number"
        },
        {
          "name": "Number of Batteries",
          "type": "number"
        },
        {
          "name": "Weight",
          "type": "number"
        },
        {
          "name": "Drivetrain",
          "type": "dropdown",
          "options": [
            "West Coast",
            "Swerve",
            "Butterfly",
            "Mecanum",
            "Tread",
            "Tank",
            "Other"
          ]
        },
        {
          "name": "Swerve Motor Type",
          "type": "dropdown",
          "options": [
            "Neo",
            "Falcon",
            "Cim",
            "Kraken",
            "Other"
          ]
        },
        {
          "name": "Swerve Type",
          "type": "dropdown",
          "options": [
            "No swerve",
            "Mk 4",
            "Mk 4i",
            "Custom"
          ]
        },
        {"name": "Speed",
         "type": "dropdown",
         "options": [
            "Can't Move",
            "Slow",
            "Moderate",
            "Fast",
            "Other(type into textbox)"
         ]
        },
        {"name": "Speed(exact)",
         "type": "text"
        }
      ]
    },



    {
      "name": "Match Play",
      "widgets": [

        {
          "name": "Can Pick Up",
          "type": "multicheckbox",
          "colspan": 3,
          "options": [
            "Algae from Ground",
            "Algae from reef",
            "Coral from ground",
            "Coral(Horizontal)"
          ]
        },
        {
          "name": "Can Score In",
          "type": "multicheckbox",
          "colspan" : 3,
          "rowspan": 3,
          "options": [
            "Reef",
            "Processer",
            "Barge"
          ]
        },
        {
          "name": "Preferred Pickup Location",
          "type": "dropdown",
          "options": [
            "Ground",
            "Coral Station"
          ],
          "defaultOption": true
        },
        {
          "name": "Preferred Scoring Location",
          "type": "dropdown",
          "options": [
            "Reef",
            "Processer",
            "Barge"
          ],
          "defaultOption": true
        },
        {
          "name": "Preferred Hanging Location",
          "type": "dropdown",
          "options": [
            "Shallow",
            "Deep"
          ],
          "defaultOption": true
        },
        {
          "name": "Cycle Time Reef",
          "type": "number"
        },
        {
          "name": "Cycle Time Processer",
          "type": "number"
        },
        {
          "name": "Cycle Time Barge",
          "type": "number"
        },
        {
          "name": "How often can the human player score into barge(scale of 1-10)?",
          "type": "spinbox",
          "max": 10
        }
      ]
    },

    {
      "name": "How good is it at...",
      "widgets": [

        {
          "name": "Picking up Algae?",
