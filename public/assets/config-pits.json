{
  "heading": "REBUILT - 2026 (Pit Scouting)",
  "logo": "test_scouting_logo.png",
  "skipTeamSelection": true,
  "pages": [
    {
      "name": "Team",
      "widgets": [
        {
          "name": "Team Name",
          "type": "text"
        },
        {
          "name": "Team Number",
          "type": "number"
        }
      ]
    },




    {
      "name": "General Robot Info",
      "widgets": [

        {
          "name": "Height",
          "type": "number"
        },
        {
          "name": "Length",
          "type": "number"
        },
        {
          "name": "Width",
          "type": "number"
        },
        {
          "name": "Number of Batteries",
          "type": "number"
        },
        {
          "name": "Weight",
          "type": "number"
        },
        {
          "name": "Drivetrain",
          "type": "dropdown",
          "options": [
            "West Coast",
            "Swerve",
            "Butterfly",
            "Mecanum",
            "Tread",
            "Tank",
            "Other"
          ]
        },
        {"name": "Speed",
         "type": "dropdown",
         "options": [
            "Can't Move",
            "Slow",
            "Moderate",
            "Fast",
            "Other(type into textbox)"
         ]
        },
        {"name": "Speed(exact)",
         "type": "text"
        }
      ]
    },

    {
      "name": "Robot Collection and Climbing Mechanisms",
      "widgets": [
        {
          "name": "Fuel Collection Mechanism",
          "type": "radio",
          "options": [
            "Feeds From Ground",
            "Relies on Outpost",
            "Other",
            "None"
          ]
        },
        {
          "name": "Climbing Mechanism",
          "type": "radio",
          "options": [
            "Jump",
            "Two-Climber Lift",
            "Ladder Climb",
            "None"
          ]
        },
        {
          "name": "Maximum Fuel Capacity",
          "type": "number"
        }
      ]
    },

    {
      "name": "Match Play",
      "widgets": [

        {
          "name": "Fuel Collection Methods ",
          "type": "multicheckbox",
          "colspan": 3,
          "options": [
            "Fuel from FIELD",
            "Fuel from DEPOT",
            "Fuel from OUTPOST",
            "Can't Collect Fuel"
          ]
        },
        {
          "name": "Preferred Collection Location",
          "type": "radio",
          "options": [
            "FIELD",
            "DEPOT",
            "OUTPOST",
            "N/A"
          ]
        },
        {
          "name": "Highest Hanging Level",
          "type": "dropdown",
          "options": [
            "Level 1",
            "Level 2",
            "Level 3"
          ]
        },
        {
          "name": "Hub Cycle Time(b/sec)",
          "type": "number"
        },
        {
          "name": "Fuel Collection Cycle Time(b/sec)",
          "type": "number"
        }

      ]
    },

